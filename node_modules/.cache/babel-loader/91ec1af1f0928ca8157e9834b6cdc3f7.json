app\">\n    <div class=\"row\">\n      <h1 class=\"center-align\">Pokedex</h1>\n    </div>\n    <div class=\"row\">\n      <div v-for=\"pokemon in pokemons\" :key=\"pokemon.id\" class=\"col s1 m2\">\n        <div id=\"pokeCards\" class=\"card\">\n          <div class=\"card-image\">\n            <img class=\"responsive-img\" :src=\"pokemon.sprites.front_default\" />\n          </div>\n          <div class=\"card-content black-text\">\n            <p>{{ `#${pokemon.id}.  ${pokemon.name}` }}</p>\n          </div>\n          <div id=\"linkDetails\" class=\"card-action\">\n            <a id=\"pokeDetails\" href=\"#\"> <p>View Poke Details</p></a>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios/dist/axios';\nimport M from 'materialize-css';\n\nexport default {\n  name: 'Pokemon',\n  data() {\n    return {\n      pokemons: [],\n      url: 'http://pokeapi.co/api/v2/pokemon?limit=151',\n    };\n  },\n  mounted() {\n    M.AutoInit();\n  },\n  methods: {\n    getAllPoke: async function() {\n      await axios\n        .get(this.url)\n        .then((response) => {\n          return response.data.results;\n        })\n        .then((results) => {\n          return Promise.all(results.map((res) => axios.get(res.url)));\n        })\n        .then((results) => {\n          this.pokemons = results.map((res) => res.data);\n          this.pokemons;\n        })\n        .catch(() => {});\n    },\n  },\n  computed: {},\n  created() {\n    this.getAllPoke();\n  },\n};\n</script>\n\n<style>\n@import url('https://fonts.googleapis.com/css2?family=Press+Start+2P&display=swap');\nbody {\n  font-family: 'Press Start 2P', cursive;\n  background-color: #141518;\n}\nh1 {\n  color: #ef5350;\n}\n#linkDetails {\n  font-size: 10px;\n  text-align: center;\n}\n#linkDetails a p {\n  color: #607d8b;\n}\n#pokeDetails:hover {\n  background-color: yellow;\n}\n#pokeCards {\n  font-size: 14px;\n  background-color: #ef9a9a;\n}\n</style>\n"],"sourceRoot":"src/components"}]}