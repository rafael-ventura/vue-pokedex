i/v2/pokemon?limit=151',\n    };\n  },\n  methods: {\n    getAllPoke() {\n      axios\n        .get(this.url)\n        .then((response) => {\n          return response.data.results;\n        })\n        .then((results) => {\n          return Promise.all(results.map((res) => axios.get(res.url)));\n        })\n        .then((results) => {\n          this.pokemons = results.map((res) => res.data);\n          return this.pokemons;\n        })\n        .catch(() => {});\n    },\n  },\n  computed: {},\n  created() {\n    this.getAllPoke();\n  },\n};\n</script>\n\n<style>\nli {\n  align-items: center;\n  list-style: none;\n  orientation: horizontal;\n}\n</style>\n"]}]}